function scp_get_ability_element(argument0) {
	//get ability mp
	//returns the mp of ability in argument0
	//argument0=ability name - need ""
	if(argument0=="Fire"){return("Fire");}
	if(argument0=="Water"){return("Water");}
	if(argument0=="Ice"){return("Ice");}
	if(argument0=="Thunder"){return("Thunder");}
	if(argument0=="Shockwave"){return("Thunder");}
	if(argument0=="Cure"){return("Holy");}



}
