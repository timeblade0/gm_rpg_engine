function scp_get_item_price(argument0) {
	//get item price
	//returns the price of the item entered
	//argument0=item name - need ""

	if(argument0==""){return(0);}
	if(argument0=="Potion"){return(50);}
	if(argument0=="Mega Potion"){return(90);}
	if(argument0=="Ether"){return(100);}
	if(argument0=="Phoenix Down"){return(300);}



}
