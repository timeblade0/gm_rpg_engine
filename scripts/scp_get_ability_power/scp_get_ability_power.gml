function scp_get_ability_power(argument0) {
	//get ability power
	//returns the power of ability in argument0
	//used for abilites that do damage
	//argument0=ability name - need ""
	if(argument0=="Fire"){return(10);}
	if(argument0=="Water"){return(10);}
	if(argument0=="Ice"){return(15);}
	if(argument0=="Thunder"){return(20);}
	if(argument0=="Cure"){return(30);}



}
