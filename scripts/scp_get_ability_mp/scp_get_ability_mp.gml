function scp_get_ability_mp(argument0) {
	//get ability mp
	//returns the mp of ability in argument0
	//argument0=ability name - need ""
	if(argument0=="Fire"){return(5);}
	if(argument0=="Water"){return(5);}
	if(argument0=="Ice"){return(7);}
	if(argument0=="Thunder"){return(10);}
	if(argument0=="Shockwave"){return(10);}
	if(argument0=="Cure"){return(5);}



}
